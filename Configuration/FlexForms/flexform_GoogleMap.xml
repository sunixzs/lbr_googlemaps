<T3DataStructure>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>Einstellungen</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.GoogleMap.locationUidList>
						<TCEforms>
							<exclude>1</exclude>
							<label>Locations</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>tx_lbrgooglemaps_domain_model_location</allowed>
								<foreign_table>tx_lbrgooglemaps_domain_model_location</foreign_table>
								<foreign_table_where>ORDER BY tx_lbrgooglemaps_domain_model_location.title
								</foreign_table_where>
								<size>10</size>
								<maxitems>100</maxitems>
								<minitems>1</minitems>
								<autoSizeMax>30</autoSizeMax>
								<multiple>0</multiple>
								<wizards>
									<_VERTICAL>1</_VERTICAL>
									<suggest>
										<type>suggest</type>
									</suggest>
								</wizards>
							</config>
						</TCEforms>
					</settings.GoogleMap.locationUidList>
					<settings.GoogleMap.apiKey>
						<TCEforms>
							<exclude>1</exclude>
							<label>Google API-Key (https://console.developers.google.com/apis/)</label>
							<config>
								<type>input</type>
								<size>60</size>
								<eval>trim,required</eval>
							</config>
						</TCEforms>
					</settings.GoogleMap.apiKey>
					<settings.GoogleMap.zoom>
						<TCEforms>
							<exclude>1</exclude>
							<label>Zoom (5-20)</label>
							<config>
								<type>input</type>
								<size>3</size>
								<eval>int</eval>
								<default>9</default>
							</config>
						</TCEforms>
					</settings.GoogleMap.zoom>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>
